				"uid" : {
					"__type" : "uid",
					"uidString" : "urzWlHzjDQC72vRHCPbF3A=="
				},
				"code" : "view := GtMondrian new.\rview nodes\r\tshape: [ :obj | \r\t\tBlElement new\r\t\t\tgeometry: BlCircle new;\r\t\t\tbackground: Color blue ];\r\twith: (1 to: 9).\rview edges connectFrom: [ :x | x // 2 ].\rview layout tree.\rview"
			},
